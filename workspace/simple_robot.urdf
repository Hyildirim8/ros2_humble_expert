  </link>

  <joint name="govde_tekerlek_baglantisi" type="continuous">
    <parent link="base_link"/>
    <child link="tekerlek"/>
    <origin xyz="0.2 0.25 0" rpy="1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>
</robot>
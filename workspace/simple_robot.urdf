<?xml version="1.0"?>
<robot name="basit_robot">
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.6 0.4 0.2"/>
      </geometry>
    </visual>
  </link>

  <link name="tekerlek">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
